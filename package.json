{
    "name": "alpine",
    "version": "0.0.1",
    "description": "API and web portal",
    "scripts": {
        "prepare": "husky install",
        "dev:next": "next dev -p 3001",
        "dev": "concurrently \"nodemon -L -x tsoa spec-and-routes\" \"nodemon -L\" ",
        "pm2:start": "pm2 start ecosystem.config.js",
        "pm2:stop": "pm2 stop ecosystem.config.js",
        "start": "npm run build && npm run pm2:start",
        "stop": "npm run pm2:stop",
        "routes": "tsoa spec-and-routes",
        "build": "tsoa spec-and-routes && npm run build:next && npm run build:server",
        "build:server": "tsc -p tsconfig.server.json",
        "build:next": "next build",
        "test": "jest --forceExit --coverage --verbose --runInBand --detectOpenHandles --updateSnapshot",
        "format": "eslint --fix server/**/*.ts src/**/*.ts src/**/*.tsx",
        "format:test": "eslint --fix test/**/*.ts",
        "lint": "prettier --write server/**/*.ts src/**/*.ts src/**/*.tsx",
        "next:lint": "next lint"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "keywords": [
        "Alpine",
        "Bulk",
        "Backend",
        "API",
        "Typescript",
        "PM2",
        "Jest",
        "Swagger"
